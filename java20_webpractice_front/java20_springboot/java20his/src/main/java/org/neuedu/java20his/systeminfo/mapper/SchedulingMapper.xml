    select sche.id,scheddate,d.id as did,deptname,u.id as uid,realname,noon,rl.id as rlid,registname,registquota
    from scheduling sche
    join department d
    on sche.deptid = d.id
    join user u
    on sche.userid = u.ID
    join registlevel rl
    on u.RegistLeID = rl.id
    <where>
      <if test="(startDate == null or startDate == 'null' or startDate == '') and (endingDate != null and endingDate != 'null' and endingDate != '')">
        scheddate &lt;= #{endingDate}
      </if>
      <if test="(endingDate == null or endingDate == 'null' or endingDate == '') and (startDate != null and startDate != 'null' and startDate != '')">
        scheddate &gt;= #{startDate}
      </if>
      <if test="(endingDate != null and endingDate != 'null' and endingDate != '') and (startDate != null and startDate != 'null' and startDate != '')">
        scheddate between #{startDate} and #{endingDate}
      </if>
    </where>
    ORDER BY scheddate desc
  </select>
